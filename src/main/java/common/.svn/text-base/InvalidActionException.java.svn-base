package common;


/**
 * Indica che l'azione scelta non puo' essere attuata nell'attuale stato di gioco
 * @author Daniele Iamartino, Daniele Gravina
 *
 */
public class InvalidActionException extends Exception {
	
	/**
	 * 
	 */
	private static final long serialVersionUID = -7965989585453613635L;

	public InvalidActionException(){
		super();
	}
	
	@Override
	public String toString(){
		return "Invalid action";
	}

}
